	<!-- 根据站点名称查询所有符合条件的数据 -->
	<select id="selectYanJiuQuWQBySiteName" resultMap="YanJiuQuWQMapper" parameterType="string">
		select * from tb_yanjiuquWQ where siteName = #{siteName}
	</select>
	
</mapper>